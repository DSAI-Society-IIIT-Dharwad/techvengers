{
  "name": "network-security-dashboard",
  "version": "1.0.0",
  "description": "Network Traffic Analyzer with ML-based threat detection",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server.js",
    "real": "node real_server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "socket.io": "^4.7.2",
    "concurrently": "^8.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": ["network", "security", "ml", "dashboard"],
  "author": "Network Security Team",
  "license": "MIT"
}
